<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	package="edu.mch.shikaku">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_shikaku"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_shikaku_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.Shikaku"
		tools:ignore="AllowBackup">
		<activity android:name=".activities.SoundTestActivity" />
		<activity android:name=".activities.ChooseDimensionsActivity" />
		<activity android:name=".activities.ChooseFieldValueActivity" />
		<activity android:name=".activities.ListActivity" />
		<activity android:name=".activities.GameActivity" />
		<activity android:name=".activities.AboutActivity" />
		<activity android:name=".activities.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>
